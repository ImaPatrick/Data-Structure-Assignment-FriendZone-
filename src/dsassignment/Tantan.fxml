<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #e66a34;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dsassignment.TantanController">
   <children>
      <Label layoutX="218.0" layoutY="83.0" text="Register with Tantan now!" textFill="WHITE">
